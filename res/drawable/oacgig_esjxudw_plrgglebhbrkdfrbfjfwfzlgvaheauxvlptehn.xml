<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff94434e" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item>
        <layer-list>
            <item>
                <shape>
                    <solid android:color="#ff9d3d45" />
                    <padding android:left="0.5dip" android:top="0.5dip" android:right="0.5dip" android:bottom="2.0dip" />
                    <corners android:radius="5.0dip" />
                </shape>
            </item>
            <item>
                <shape>
                    <solid android:color="#ffb7545f" />
                    <padding android:left="0.5dip" android:top="0.5dip" android:right="0.5dip" android:bottom="1.0dip" />
                    <corners android:radius="5.0dip" />
                </shape>
            </item>
            <item>
                <shape>
                    <gradient android:startColor="#ffde6a8c" android:endColor="#ffcd4057" android:angle="-90.0" />
                    <corners android:radius="5.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>