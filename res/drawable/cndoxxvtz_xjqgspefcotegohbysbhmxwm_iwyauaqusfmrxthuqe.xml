<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff3062b4" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item>
        <layer-list>
            <item>
                <shape>
                    <solid android:color="#ff3269c8" />
                    <padding android:left="0.5dip" android:top="0.5dip" android:right="0.5dip" android:bottom="2.0dip" />
                    <corners android:radius="5.0dip" />
                </shape>
            </item>
            <item>
                <shape>
                    <solid android:color="#ff4383e7" />
                    <padding android:left="0.5dip" android:top="0.5dip" android:right="0.5dip" android:bottom="1.0dip" />
                    <corners android:radius="5.0dip" />
                </shape>
            </item>
            <item>
                <shape>
                    <gradient android:startColor="#ff3f7ce0" android:endColor="#ff3269c8" android:angle="-90.0" android:centerColor="#ff3875d5" />
                    <corners android:radius="5.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>