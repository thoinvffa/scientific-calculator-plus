<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff706f6e" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item>
        <layer-list>
            <item>
                <shape>
                    <solid android:color="#ff706f6e" />
                    <padding android:left="0.5dip" android:top="0.5dip" android:right="0.5dip" android:bottom="0.5dip" />
                    <corners android:radius="5.0dip" />
                </shape>
            </item>
            <item>
                <shape>
                    <solid android:color="#ff777a7a" />
                    <padding android:left="0.5dip" android:top="0.5dip" android:right="0.5dip" android:bottom="0.5dip" />
                    <corners android:radius="5.0dip" />
                </shape>
            </item>
            <item>
                <shape>
                    <gradient android:startColor="#ff8d8b8b" android:endColor="#ff646662" android:angle="-90.0" android:centerColor="#ff797777" />
                    <corners android:radius="5.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>