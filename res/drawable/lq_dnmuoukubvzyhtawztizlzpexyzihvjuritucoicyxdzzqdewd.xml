<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <corners android:radius="6.0dip" />
                    <solid android:color="#ff0f0c0d" />
                    <padding android:left="1.75dip" android:top="1.75dip" android:right="1.75dip" android:bottom="1.75dip" />
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <corners android:radius="5.0dip" />
                    <solid android:color="#ff807d82" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item>
                <shape android:shape="rectangle">
