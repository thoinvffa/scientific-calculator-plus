<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="6.0dip" />
            <solid android:color="#ff141614" />
            <padding android:left="1.0dip" android:top="1.0dip" android:right="1.0dip" android:bottom="1.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ff9f9da0" android:endColor="#fff5f7f7" android:angle="90.0" android:type="linear" android:centerColor="#ffe2dee0" />
            <padding android:left="1.5dip" android:top="1.5dip" android:right="1.5dip" android:bottom="1.5dip" />
        </shape>
    </item>
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="rectangle">
                    <solid android:color="@color/material_grey_200" />
                    <corners android:radius="4.0dip" />
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/material_grey_400" />
                    <corners android:radius="3.0dip" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>